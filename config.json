  "instance_version": "6.14.0",
  "task_location": "workspace_tasks",
  "isolate": true,
  "headless": false,
  "need_gpu": false,
  "context_menu": {
    "target": ["videos_project", "images_project", "images_dataset", "videos_dataset"],
    "context_root": "Neural Networks"
  }
}
