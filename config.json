{
  "name": "Apply Neural Networks",
  "type": "app",
  "version": "2.0.0",
  "categories": [
    "neural network",
    "images",
    "videos",
    "detection & tracking",
    "segmentation & tracking",
    "object detection",
    "semantic segmentation",
    "instance segmentation",
    "inference interfaces",
    "labeling"
  ],
  "entrypoint": "python -m uvicorn src.main:app --host 0.0.0.0 --port 8000",
